{
  "name": "ts-gunengine",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "dev:back": "nodemon back/index.ts",
    "dev:front": "tsc-watch -p tsconfig.front.json",
    "dev": "yarn run clean && concurrently \"yarn run dev:back\" \"yarn run dev:front\""
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.11",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
