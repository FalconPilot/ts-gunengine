{
  "name": "ts-gunengine",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "shx rm -rf dist",
    "dev:back": "nodemon src/back/index.ts",
    "dev:front": "webpack --watch",
    "dev": "yarn run clean && concurrently \"yarn run dev:back\" \"yarn run dev:front\"",
    "build:back": "tsc && shx cp -r src/back/static dist/back/static",
    "build:front": "webpack --mode=production",
    "build": "yarn run clean && yarn run build:front && yarn run build:back",
    "start": "node -r tsconfig-paths/register -r ts-node/register dist/back/index.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3",
    "shx": "^0.3.3",
    "styled-components": "^5.2.3",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.11",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7",
    "ts-loader": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  }
}
